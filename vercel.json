{
  "build": {
    "env": {
      "UNSPLASH_APP_SECRET": "@unsplash_app_secret"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/dist" },
    { "source": "/assets/socialsharing.png", "destination": "/api/open-graph-image" }
  ],
  "redirects": [
    { "source": "/xing", "destination": "https://www.xing.com/profile/Stefan_Schult2", "permanent": false },
    { "source": "/twitter", "destination": "https://twitter.com/schubidu", "permanent": false },
    { "source": "/github", "destination": "https://github.com/Schubidu", "permanent": false },
    { "source": "/linkedin", "destination": "https://www.linkedin.com/in/stefan-schult-9185261a1", "permanent": false }
  ]
}
